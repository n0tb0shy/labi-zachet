// Здесь мы объявляем переменные x, y и z, которые будут вводиться пользователем.
using System;
int x = 0;
int y = 0;
int z = 0;

// Запрос у пользователя ввода значения для x
Console.Write("x: ");
x = Convert.ToInt32(Console.ReadLine());

// Здесь мы проверяем, является ли введенное значение для x равным нулю
if (x == 0)
{
    throw new Exception("x can't be equal to null");
}

// Запрос у пользователя ввода значения для y.
Console.Write("y: ");
y = Convert.ToInt32(Console.ReadLine());

// Здесь мы проверяем, является ли введенное значение для y равным нулю
if (y == 0)
{
    throw new Exception("y can't be equal to null");
}

// Вычисление значения выражения z=x/y.
z = x / y;

// Вывод результата.
Console.WriteLine($"z=x/y: {z}");
